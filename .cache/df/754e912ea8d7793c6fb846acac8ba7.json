        this.mapRegions(templateElement.content)\n        this.onRender();\n\n        this.parent.appendChild(templateElement.content)\n    }\n}"},"lineCount":null}},"error":null,"hash":"0526ec5d68fec8fd7340a0093004cb6f","cacheData":{"env":{}}}